<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <annotator language="VueJS" implementationClass="com.eny.i18n.vue.plugin.ide.annotator.VueCompositeKeyAnnotator"/>
        <psi.referenceContributor language="VueJS" implementation="com.eny.i18n.vue.plugin.ide.references.code.VueReferenceContributor"/>
        <completion.contributor language="VueJS" implementationClass="com.eny.i18n.vue.plugin.ide.completion.VueCompletionContributor"/>
        <lang.foldingBuilder language="HTML" implementationClass="com.eny.i18n.vue.plugin.ide.folding.VueJsFoldingBuilder"/>
    </extensions>
</idea-plugin>